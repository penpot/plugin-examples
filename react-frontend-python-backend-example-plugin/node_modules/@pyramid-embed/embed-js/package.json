  "license": "ISC",
  "description": "",
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "ts-loader": "8.3.0",
    "typescript": "5.4.2",
    "webpack": "5.90.3",
    "webpack-cli": "5.1.4",
    "webpack-merge": "5.10.0"
  },
  "files": [
    "dist/**/*"
  ],
  "private": false
